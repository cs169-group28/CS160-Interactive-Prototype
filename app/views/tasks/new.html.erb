<h1>Create a New Task</h1>
<%= form_for([@event, @task], :url => event_tasks_path) do |f| %>
	<%= render :partial => 'form', :locals => {:f => f} %>
<% end %>

<button class="btn btn-primary" data-dismiss="modal" aria-hidden="true" style="margin-top:-10px;margin-right:10px;float:right">Back to Tasks</button>
